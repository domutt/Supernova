        </div>
        <div id="sub-menu-container">
            <div id="sub-menu-lists">
                <div id="menu-1" class="nav-lists">
                    {{#each aboutmenu}}
                    <div class="nav-list-container">
                        <span class="nav-list-item">{{this}}</span>
                    </div>
                    {{/each}}
                </div>
                <div id="menu-2" class="nav-lists">
                    {{#each schedmenu}}
                    <div class="nav-list-container">
                        <span class="nav-list-item">{{this}}</span>
                    </div>
                    {{/each}}
                </div>
            </div>

        </div>
    </div>
