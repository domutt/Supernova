    <div id="page-header">
        <div id="supernova-image">
            <img src="../images/supernova-long.png" alt="Supernova Logo">
        </div>
        <div id="menu">
            <div id="menu-items">
                <button id="current-year-button" onclick="toggleSched();" >ABOUT</button>
                <button href="#" id="main-menu-button" onclick="toggleAbout();">SCHEDULE</button>
            </div>
        </div>
        <div id="banner">
